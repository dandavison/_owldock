{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/manage.py",
      "args": [
        "runserver",
        "--settings",
        "owldock.settings.dev",
        "0.0.0.0:8000",
        "2>/tmp/stderr"
      ],
      "envFile": "${workspaceFolder}/.env.dev",
      "django": true,
      "justMyCode": false
    }
  ]
}
